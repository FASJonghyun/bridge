{
    "rewrites": [{ "source": "/(.*)", "destination": "/" }],
    "builds": [
        {
            "src": "fas_bridge/package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
    { "src": "/[^.]+", "dest": "/", "status": 200 }
    ],
    "outputDirectory": "fas_bridge/.next"
  }
  